{
  "global": {
    "color": {
      "brand": {
        "50": {
          "$type": "color",
          "$value": "#E6F0FF"
        },
        "100": {
          "$type": "color",
          "$value": "#B3D1FF"
        },
        "500": {
          "$type": "color",
          "$value": "#0066FF"
        },
        "600": {
          "$type": "color",
          "$value": "#0052CC"
        },
        "700": {
          "$type": "color",
          "$value": "#003D99"
        }
      },
      "orange": {
        "500": {
          "$type": "color",
          "$value": "#FF6600"
        },
        "600": {
          "$type": "color",
          "$value": "#CC5200"
        }
      },
      "gray": {
        "50": {
          "$type": "color",
          "$value": "#F9FAFB"
        },
        "100": {
          "$type": "color",
          "$value": "#F3F4F6"
        },
        "300": {
          "$type": "color",
          "$value": "#D1D5DB"
        },
        "500": {
          "$type": "color",
          "$value": "#6B7280"
        },
        "900": {
          "$type": "color",
          "$value": "#111827"
        }
      },
      "semantic": {
        "success": {
          "$type": "color",
          "$value": "#16A34A"
        },
        "error": {
          "$type": "color",
          "$value": "#DC2626"
        },
        "warning": {
          "$type": "color",
          "$value": "#F59E0B"
        }
      }
    },
    "spacing": {
      "xs": {
        "$type": "spacing",
        "$value": "4px"
      },
      "sm": {
        "$type": "spacing",
        "$value": "8px"
      },
      "md": {
        "$type": "spacing",
        "$value": "16px"
      },
      "lg": {
        "$type": "spacing",
        "$value": "24px"
      },
      "xl": {
        "$type": "spacing",
        "$value": "32px"
      }
    },
    "radius": {
      "sm": {
        "$type": "borderRadius",
        "$value": "4px"
      },
      "md": {
        "$type": "borderRadius",
        "$value": "8px"
      },
      "lg": {
        "$type": "borderRadius",
        "$value": "16px"
      },
      "full": {
        "$type": "borderRadius",
        "$value": "9999px"
      }
    },
    "typography": {
      "fontFamily": {
        "base": {
          "$type": "fontFamilies",
          "$value": "Inter, sans-serif"
        }
      },
      "fontSize": {
        "xs": {
          "$type": "fontSizes",
          "$value": "12px"
        },
        "sm": {
          "$type": "fontSizes",
          "$value": "14px"
        },
        "md": {
          "$type": "fontSizes",
          "$value": "16px"
        },
        "lg": {
          "$type": "fontSizes",
          "$value": "20px"
        },
        "xl": {
          "$type": "fontSizes",
          "$value": "24px"
        }
      },
      "fontWeight": {
        "regular": {
          "$type": "fontWeights",
          "$value": "400"
        },
        "medium": {
          "$type": "fontWeights",
          "$value": "500"
        },
        "bold": {
          "$type": "fontWeights",
          "$value": "700"
        }
      },
      "lineHeight": {
        "normal": {
          "$type": "lineHeights",
          "$value": "1.5"
        },
        "heading": {
          "$type": "lineHeights",
          "$value": "1.2"
        }
      }
    },
    "shadow": {
      "sm": {
        "$type": "boxShadow",
        "$value": "0 1px 2px rgba(0,0,0,0.05)"
      },
      "md": {
        "$type": "boxShadow",
        "$value": "0 4px 6px rgba(0,0,0,0.1)"
      }
    }
  },
  "alias": {
    "color": {
      "primary": {
        "$type": "color",
        "$value": "{global.color.brand.500}"
      },
      "primaryHover": {
        "$type": "color",
        "$value": "{global.color.brand.600}"
      },
      "secondary": {
        "$type": "color",
        "$value": "{global.color.orange.500}"
      },
      "background": {
        "$type": "color",
        "$value": "{global.color.gray.50}"
      },
      "textPrimary": {
        "$type": "color",
        "$value": "{global.color.gray.900}"
      },
      "border": {
        "$type": "color",
        "$value": "{global.color.gray.300}"
      }
    }
  },
  "component": {
    "button": {
      "primary": {
        "background": {
          "$type": "color",
          "$value": "{alias.color.primary}"
        },
        "backgroundHover": {
          "$type": "color",
          "$value": "{alias.color.primaryHover}"
        },
        "textColor": {
          "$type": "color",
          "$value": "#FFFFFF"
        },
        "padding": {
          "$type": "spacing",
          "$value": "{global.spacing.md}"
        },
        "radius": {
          "$type": "borderRadius",
          "$value": "{global.radius.md}"
        }
      }
    },
    "input": {
      "background": {
        "$type": "color",
        "$value": "#FFFFFF"
      },
      "borderColor": {
        "$type": "color",
        "$value": "{alias.color.border}"
      },
      "padding": {
        "$type": "spacing",
        "$value": "{global.spacing.sm}"
      },
      "radius": {
        "$type": "borderRadius",
        "$value": "{global.radius.sm}"
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global",
      "alias",
      "component"
    ]
  }
}